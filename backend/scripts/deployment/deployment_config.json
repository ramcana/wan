{
    "deployment_settings": {
        "setup_monitoring": true,
        "cleanup_after_deployment": true,
        "auto_rollback_on_failure": true,
        "validation_timeout_seconds": 300,
        "migration_timeout_seconds": 600,
        "deployment_timeout_seconds": 1800
    },
    "validation_settings": {
        "skip_non_critical_checks": false,
        "require_all_components": true,
        "check_disk_space_gb": 5,
        "check_memory_usage_percent": 85,
        "check_cpu_usage_percent": 80
    },
    "backup_settings": {
        "create_pre_deployment_backup": true,
        "backup_type": "full_system",
        "keep_backup_count": 10,
        "backup_retention_days": 30
    },
    "migration_settings": {
        "migrate_existing_models": true,
        "verify_model_integrity": true,
        "create_model_metadata": true,
        "backup_original_models": true
    },
    "monitoring_settings": {
        "enable_real_time_monitoring": true,
        "monitoring_interval_seconds": 30,
        "alert_on_critical_issues": true,
        "alert_on_warnings": false,
        "metrics_retention_hours": 168
    },
    "rollback_settings": {
        "auto_rollback_on_critical_failure": true,
        "rollback_timeout_seconds": 600,
        "verify_rollback_success": true,
        "keep_rollback_points": 5
    },
    "notification_settings": {
        "enable_console_notifications": true,
        "enable_log_notifications": true,
        "enable_file_notifications": true,
        "notification_levels": [
            "critical",
            "warning",
            "info"
        ]
    },
    "performance_settings": {
        "parallel_operations": true,
        "max_concurrent_operations": 3,
        "operation_timeout_seconds": 300,
        "retry_failed_operations": true,
        "max_retry_attempts": 3
    }
}