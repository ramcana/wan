# Example environment variables for WAN
API_HOST=0.0.0.0
API_PORT=8000
DATABASE_URL=postgresql+psycopg2://postgres:postgres@postgres:5432/wan
DATABASE_POOL_SIZE=10
MODEL_CACHE_DIR=models
DEFAULT_MODEL=T2V-A14B
QUANTIZATION_MODE=fp16
OUTPUTS_DIR=backend/outputs
TEMP_DIR=backend/temp
MAX_STORAGE_GB=50
LOG_LEVEL=INFO
LOG_FILE=backend/logs/app.log
MAX_WORKERS=4
WORKER_TIMEOUT=300
SECRET_KEY=change_me
JWT_SECRET=change_me_too

# Queue
REDIS_URL=redis://redis:6379/0
