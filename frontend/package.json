{
    "name": "wan22-react-frontend",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:unit": "vitest run src/tests/unit",
        "test:integration": "vitest run src/tests/integration",
        "test:e2e": "vitest run src/tests/e2e",
        "test:performance": "vitest run src/tests/performance",
        "test:comprehensive": "vitest run src/tests/comprehensive-test-suite.ts",
        "test:watch": "vitest",
        "test:integration:full": "vitest run src/tests/integration --reporter=verbose",
        "coverage": "vitest run --coverage",
        "analyze-bundle": "vite build --mode analyze",
        "performance:validate": "npm run build && npm run test:performance"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@hookform/resolvers": "^5.2.1",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slider": "^1.3.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.0.3",
        "@radix-ui/react-toast": "^1.1.5",
        "axios": "^1.6.2",
        "chart.js": "^4.4.0",
        "chartjs-adapter-date-fns": "^3.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.0.0",
        "date-fns": "^4.1.0",
        "framer-motion": "^12.23.12",
        "lucide-react": "^0.294.0",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.48.2",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.20.1",
        "tailwind-merge": "^2.0.0",
        "zod": "^3.22.4",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.1.1",
        "@vitest/ui": "^0.34.7",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "jsdom": "^26.1.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vitest": "^0.34.6"
    }
}
