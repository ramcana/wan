{
  "environment": "development",
  "monitoring_enabled": true,
  "health_checks": {
    "interval_seconds": 60,
    "endpoints": [
      "/health",
      "/metrics",
      "/status"
    ]
  },
  "alerts": {
    "email_enabled": false,
    "log_enabled": true,
    "thresholds": {
      "error_rate": 0.1,
      "response_time_ms": 5000
    }
  }
}